        "def isHappy(a) :\n",
        "    s = 0\n",
        "    while a//1 != 0 :\n",
        "        s += (a%10)**2\n",
        "        a = int(a/10)\n",
        "    return (s)\n",
        "\n",
        "a = int(input())\n",
        "\n",
        "while 1 :\n",
        "    a = isHappy(a)\n",
        "    if a == 1 :\n",
        "        print('true')\n",
        "        break\n",
        "    elif a == 4 :\n",
        "      print(\"false\")\n",
        "      break\n",
        "\n"
      ]
